{
  "env": {
    "browser": true
  },
  "extends": [
    "airbnb-base"
  ],
  "rules": {
    "no-plusplus": "off",
    "no-underscore-dangle": ["error", { "allowAfterThis": true }],
    "no-param-reassign": ["error", { "props": false }],
    "import/prefer-default-export": "off",
    "no-mixed-operators": "off"
  }
}
